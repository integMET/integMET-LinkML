        :samplingLocation           "Japan:Chiba, Chiba University" ;
        :samplingTime               "1600 (JST, Japan Standard Time),  corresponding to midday in the growth chamber" ;
        :speciesName                "<i>Solanum lycopersicum</i>" ;
        obo:RO_0002162              <http://purl.bioontology.org/ontology/NCBITAXON/4081> ;
        dcterms:identifier          "L_088_1" .

